@model StepByStep.Web.Controllers.DadosPessoaisVM
@using StepByStep.Web.Helpers
@using StepByStep.Web.Infra
@{
    ViewBag.Title = "Dados Pessoais";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="row bs-wizard">
    @Html.ProgressBarPara(User.Identity.Name)
</div>

<h2>Dados Pessoais</h2>
@using (Html.BeginForm("DadosPessoais", "Formulario", FormMethod.Post))
{

    @Html.EditorFor(m => m.Cpf, new { htmlAttributes = new { @class = "form-control" } })
    <button type="submit" class="btn btn-default btn-primary">Enviar</button>
    @Html.ValidationMessageFor(m => m.Cpf)
}